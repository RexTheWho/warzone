<dynamic_object>
    <diesel materials="units/pd2_mod_wz/buildings/bld_store/bld_store" orientation_object="rp_bld_store" />
	<sequence_manager file="units/pd2_mod_wz/buildings/bld_store/bld_store" />

    <bodies>
        <body name="static_body" enabled="true" template="static">
            <object name="g_bld_store" collision_type="mesh_mopp" />
            <object name="g_bld_store_misc" collision_type="mesh_mopp" />
            <object name="g_bld_store_misc2" collision_type="mesh_mopp" />
            <object name="g_bld_store_ladder" collision_type="mesh_mopp" />
            <object name="g_ext_front" collision_type="mesh_mopp" />
        </body>
        <body name="animated_door" enabled="true" template="animated">
			<object name="anim_store_door_open_close" />
            <object name="c_door" collision_type="box" padding="-2.5" />
        </body>
    </bodies>
	
    <graphics>
        <object enabled="true" name="g_bld_store" shadow_caster="true"/>
        <object enabled="true" name="g_bld_store_ladder" shadow_caster="true"/>
        <object enabled="true" name="g_ext_front" shadow_caster="true"/>
        <object enabled="true" name="g_door" shadow_caster="true"/>
		
		<graphic_group name="grp_interior" enabled="false" >
			<object enabled="true" name="g_bld_store_misc" shadow_caster="true"/>
			<object enabled="true" name="g_bld_store_misc2" shadow_caster="true"/>
		</graphic_group>
<!-- 		
		<lod_object name="lod_group">
				<object name="g_bld_store"		enabled="true"  max_distance="1600"  max_draw_lod="0" />
				<object name="g_bld_store_lod1"	enabled="true"  lod="1" />
		</lod_object>
		 -->
    </graphics>
	

	<lights>
		<light name="lo_store_001" enabled="true"/>
		<light name="lo_store_002" enabled="true"/>
	</lights>
	
</dynamic_object>