<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_wz/supplies/wz_supply_scrap/wz_supply_scrap" orientation_object="rp_loot_bag" />
	<sequence_manager file="units/pd2_mod_wz/supplies/wz_supply_scrap/wz_supply_scrap" />
	<bodies>
		<body  name="hinge_body_1" enabled="true" template="dynamic" collides_with_mover="false">
			<object name="rp_loot_bag" />
			<object name="c_hinge_body_1" collision_type="box" mass="50" padding="-2"/>
		</body>
		<body  name="hinge_body_2" enabled="true" template="dynamic" collides_with_mover="false">
			<object name="a_hinge_body_2" />
			<object name="c_hinge_body_2" collision_type="box" mass="50" padding="-2" />
		</body>
	</bodies>
	<constraints>
		<constraint name="bag" enabled="true" type="ragdoll">
			<param body_a="hinge_body_1" body_b="hinge_body_2"/>
			<param pivot="position:a_hinge_body_2"/>
			<param twist_axis="yaxis:a_hinge_body_2" twist_min="-5" twist_max="5" twist_freedom="2"/>
			<param plane_axis="xaxis:a_hinge_body_2"/>
			<param cone_y="50" cone_z="50" cone_freedom="2"/>
			<param damping="16" spring_constant="1000" min_restitution="0"/>
		</constraint>
	</constraints>
	<decal_surfaces default_material="cloth" />
	<graphics>
		<object name="g_bag" enabled="true"/>
	</graphics>
</dynamic_object>